    "import tclab\n",
    "import time\n",
    "# Connect to Arduino\n",
    "a = tclab.TCLab()\n",
    "print('LED On')\n",
    "a.LED(100)\n",
    "# Pause for 1 second\n",
    "time.sleep(1.0)\n",
    "print('LED Off')\n",
    "a.LED(0)\n",
    "a.close()"
